{
  "name": "haro",
  "version": "16.0.0",
  "description": "Haro is a modern immutable DataStore",
  "type": "module",
  "types": "types/haro.d.ts",
  "source": "src/haro.js",
  "main": "dist/haro.cjs",
  "exports": {
    "types": "./types/haro.d.ts",
    "import": "./dist/haro.js",
    "require": "./dist/haro.cjs"
  },
  "files": [
    "dist/haro.cjs",
    "dist/haro.js",
    "types"
  ],
  "scripts": {
    "benchmark": "node benchmarks/index.js",
    "build": "npm run lint && npm run rollup",
    "changelog": "auto-changelog -p",
    "lint": "eslint --fix *.js benchmarks/*.js src/*.js",
    "mocha": "c8 --reporter=text mocha 'tests/unit/**/*.test.js'",
    "rollup": "rollup --config",
    "test": "npm run lint && npm run mocha",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avoidwork/haro.git"
  },
  "keywords": [
    "immutable",
    "data",
    "store",
    "datastore",
    "api",
    "haro"
  ],
  "author": "Jason Mulligan <jason.mulligan@avoidwork.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/avoidwork/haro/issues"
  },
  "homepage": "https://github.com/avoidwork/haro",
  "engineStrict": true,
  "engines": {
    "node": ">=17.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-terser": "^0.4.4",
    "auto-changelog": "^2.5.0",
    "c8": "^10.1.3",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "mocha": "^11.7.1",
    "rollup": "^4.45.0"
  }
}
